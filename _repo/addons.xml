<?xml version="1.0" encoding="UTF-8"?>
<addons>
    <addon id="repository.kenshi" name="Kenshi Repo" version="1.0.0" provider-name="kenshi">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Kenshi Repo">
            <info compressed="false">https://raw.githubusercontent.com/KenshiHH/kodi/main/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/KenshiHH/kodi/main/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/KenshiHH/kodi/main/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>custom addons</summary>
            <description>own custom addons</description>
            <platform>all</platform>
    </extension>
</addon>

<addon id="script.updater.4k" name="Updater 4K" version="1.0.0" provider-name="kenshi">
	<requires>
		<import addon="xbmc.python" version="3.0.0"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Updater Serien</summary>
		<description lang="en">Updater Serien</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<forum>http://forum.kodi.tv/showthread.php?tid=209948</forum>
		<source>https://github.com/zag2me/script.hello.world</source>
		<website>Kodi.tv</website>
		<email>zag@kodi.org</email>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
		</assets>
		<news>Updated the addon to use new addon.xml metadata</news>
	</extension>
</addon>

